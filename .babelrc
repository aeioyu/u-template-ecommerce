// babel config for jest
{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    "babel-plugin-macros",
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "@": "."
        }
      }
    ]
  ]
}
